        xmlns:avconv="using:Avalonia.Controls.Converters"
        xmlns:sty="using:FluentAvalonia.Styling"
        x:Class="FluentAvalonia.Styling.FluentAvaloniaTheme">
    <Styles.Resources>
        <ResourceDictionary>
            <FontFamily x:Key="SymbolThemeFontFamily">/Fonts/#Symbols</FontFamily>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="/Styling/StylesV2/Fluentv2.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Styles.Resources>

    <StyleInclude Source="/Styling/ControlThemes/Controls.axaml" />
</Styles>
